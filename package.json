    "yeoman-generator": "4.4.0"
  },
  "devDependencies": {
    "eslint": "6.8.0",
    "eslint-config-airbnb-base": "14.0.0",
    "eslint-config-prettier": "6.9.0",
    "eslint-plugin-import": "2.19.1",
    "eslint-plugin-prettier": "3.1.2",
    "fs-extra": "8.1.0",
    "husky": "3.1.0",
    "lint-staged": "9.5.0",
    "mocha": "6.2.2",
    "prettier": "1.19.1",
    "yeoman-assert": "3.1.1",
    "yeoman-test": "2.0.0"
